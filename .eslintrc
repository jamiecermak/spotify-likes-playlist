{
    "root": true,
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "no-console": "error",
        "no-unused-vars": "warn"
    }
}
